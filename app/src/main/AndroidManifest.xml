<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yohan.blogapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UnityPost"></activity>
        <activity android:name=".PHPPost" />
        <activity android:name=".NetworkingPost" />
        <activity android:name=".LaravelPost" />
        <activity android:name=".KotlinPost" />
        <activity android:name=".HtmlPost" />
        <activity android:name=".GitPosts" />
        <activity android:name=".FiverrPost" />
        <activity android:name=".AngularPost" />
        <activity android:name=".RecentPostView" />
        <activity android:name=".JavaPost" />
        <activity android:name=".Loginaccount" />
        <activity android:name=".Register" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>